package com.demo.bean;

public class Manager extends Employee{
  
	private double incentive;

	public double getIncentive() {
		return incentive;
	}

	public void setIncentive(double incentive) {
		this.incentive = incentive;
	}

	public Manager(int empno, String name, Address location, double incentive) {
		super(empno, name, location);
		this.incentive = incentive;
	}

	public Manager(double incentive) {
		super();
		this.incentive = incentive;
	}

	@Override
	public String toString() {
		return "Manager [incentive=" + incentive + ", getEmpno()=" + getEmpno() + ", getName()=" + getName()
				+ ", getLocation()=" + getLocation() + ", toString()=" + super.toString() + ", getClass()=" + getClass()
				+ ", hashCode()=" + hashCode() + "]";
	}
	
	
	
}
