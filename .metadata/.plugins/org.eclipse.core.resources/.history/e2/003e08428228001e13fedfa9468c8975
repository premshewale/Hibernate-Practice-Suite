package com.demo.test;

import org.hibernate.Session;
import org.hibernate.SessionFactory;
import org.hibernate.Transaction;
import org.hibernate.cfg.Configuration;

import com.demo.bean.Developer;
import com.demo.bean.Employee;
import com.demo.bean.Manager;
import com.demo.bean.Product;

public class TestInsert {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		Configuration cfg=new Configuration();
		cfg.configure("hibernate.cfg.xml");
		SessionFactory sf = cfg.buildSessionFactory();
		Session s= sf.openSession();
		Transaction t=s.beginTransaction();
		Product p1=new Product("Parle G", "Biscuit", 500, 10);
		Product p2=new Product("Krack Jack", "Biscuit", 400, 20);
		Product p3=new Product("Rasana", "Soft Drink", 200, 40);
		Product p4=new Product("Ponds", "Talc", 50, 75);
		Product p5=new Product("Lux", "Soap", 30, 45);
		Product p6=new Product("Sprite", "Cold Drink", 500, 10);
		Product p7=new Product("NatureCare", "Tea", 70, 100);
		Product p8=new Product("Britania", "Biscuit", 200, 20);
		Product p9=new Product("Oreo", "Biscuit", 300, 30);
		Product p10=new Product("CocoCola", "Cold Drink", 500, 10);
		s.save(p1);
		
		t.commit();
		s.close();
		sf.close();
		System.out.println("All done check DB..");
		
	}

}
